              ],
              "hasConfig": false,
              "controls": [
                {
                  "value": {
                    "focalPoint": {
                      "left": 0.5,
                      "top": 0.5
                    },
                    "id": 1151,
                    "udi": "umb://media/7ce63d95ebc140d4a32351ec65eb8ca4",
                    "image": "/media/1033/datingfrontpage.png"
                  },
                  "editor": {
                    "alias": "media"
                  },
                  "active": false
